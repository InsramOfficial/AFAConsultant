<div id="services" class="container-fluid country overflow-hidden py-5">
    <div class="container">
        <div class="section-title text-center wow fadeInUp" data-wow-delay="0.1s" style="margin-bottom: 70px;">
            <div class="sub-style">
                <h5 class="sub-title text-primary px-3">COUNTRIES WE OFFER</h5>
            </div>
            <h1 class="display-5 mb-4">Immigration & visa services following Countries</h1>
            <p class="mb-0">
                We specialize in providing seamless visa and immigration assistance, ensuring you have the necessary support for a smooth transition to your new home or workplace. Our expert consultants help guide you through each step of the process, ensuring compliance with regulations and simplifying paperwork to make your journey stress-free.
            </p>
        </div>
        <div class="row g-4 text-center">
            @foreach (var item in Model.Countries)
            {
                <div class="col-lg-6 col-xl-3 mb-5 mb-xl-0 wow fadeInUp" data-wow-delay="0.1s">
                    <div class="country-item">
                        <div class="rounded overflow-hidden">
                            <img src="~/images/@item.Country_PicUrl" class="img-fluid w-100 rounded" alt="Image" style="height: 312px; object-fit: cover;">
                        </div>
                        <div class="country-flag">
                            <img src="~/images/@item.Flag_PicUrl" class="img-fluid rounded-circle" alt="Image">
                        </div>
                        <div class="country-name">
                            <a href="#" class="text-white fs-4">@item.Name</a>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<!-- Countries We Offer End -->

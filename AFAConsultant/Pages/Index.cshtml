                                <input asp-for="message.PhoneNumber" class="form-control" placeholder="Phone">
                                <label asp-for="message.PhoneNumber"></label>
                            </div>
                            <span asp-validation-for="message.PhoneNumber" class="text-danger"></span>
                        </div>
                        @* <div class="col-lg-12 col-xl-6">
                        <div class="form-floating">
                        <input type="text" class="form-control" id="project" placeholder="Project">
                        <label for="project">Your Project</label>
                        </div>
                        </div> *@
